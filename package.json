{
  "name": "snaptrade-node",
  "version": "1.1.1",
  "description": "The official NodeJS library for the SnapTrade API.",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsc && parcel watch --no-cache",
    "build": "tsc && parcel build --no-cache",
    "prepare": "npm run build",
    "version": "git add -A src",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/passiv/snaptrade-node-sdk"
  },
  "keywords": [
    "snaptrade",
    "snaptradeapi",
    "snaptradesdk",
    "snaptradenodesdk"
  ],
  "author": {
    "name": "Shayan Abedi",
    "email": "shayan.abedi@passiv.com",
    "url": "https://shayanabedi.com"
  },
  "license": "ISC",
  "homepage": "https://snaptrade.com",
  "bugs": {
    "url": "https://github.com/passiv/snaptrade-node-sdk/issues"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/node": "17.0.21",
    "@types/node-rsa": "^1.1.1",
    "parcel": "^2.7.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "encrypt-rsa": "^2.1.2",
    "hybrid-crypto-js": "^0.2.4",
    "node-rsa": "^1.1.1"
  }
}
